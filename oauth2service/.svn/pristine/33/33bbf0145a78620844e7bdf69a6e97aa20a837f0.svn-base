server:
  port: 8080
  servlet:
    context-path: /oauth2service
spring:
  datasource:
    driverClassName: com.mysql.jdbc.Driver
    url: jdbc:mysql://192.168.1.6:3306/oauth2service?useUnicode=true&characterEncoding=UTF-8&zeroDateTimeBehavior=convertToNull&allowMultiQueries=true
    username: root
    password: root
#用于类不能加载
  main:
    allow-bean-definition-overriding: true
#redis配置 
  redis:
    database: 0
    host: 127.0.0.1
    port: 6379
    password: 123456
    # 连接超时时间（毫秒）
    timeout: 0
    pool:
    # 连接池中的最大空闲连接，默认值也是8。
      max-idle: 500
    #连接池中的最小空闲连接，默认值也是0。
      min-idle: 50
    # 如果赋值为-1，则表示不限制；如果pool已经分配了maxActive个jedis实例，则此时pool的状态为exhausted(耗尽)。
      max-active: 2000
    # 等待可用连接的最大时间，单位毫秒，默认值为-1，表示永不超时。如果超过等待时间，则直接抛出JedisConnectionException
      max-wait: -1
# name of Redis server  哨兵监听的Redis server的名称
    sentinel:
      master: mymaster
# comma-separated list of host:port pairs  哨兵的配置列表
      nodes: 127.0.0.1:26379,127.0.0.1:26378,127.0.0.1:26377 
mybatis:
  mapper-locations: classpath:mapper/*.xml
  type-aliases-package: com.zallz.oauth2service.repository

logging:
# 你需要存储日志的目录
  file: E:/oauth2serviceLog/oauth2service.log
  level:
    org.springframework.web: DEBUG
    org.springframework.security: DEBUG
    com.interest: DEBUG
